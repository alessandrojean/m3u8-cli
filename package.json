{
  "name": "m3u8-cli",
  "description": "M3U8 IPTV playlist management system",
  "version": "1.0.0",
  "author": "Alessandro Jean @alessandrojean",
  "bin": {
    "m3u8-cli": "./bin/run"
  },
  "bugs": "https://github.com/alessandrojean/m3u8-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.19.2",
    "chalk": "^3.0.0",
    "cli-ux": "^5.2.1",
    "countries-list": "^2.4.2",
    "ejs": "^3.0.1",
    "inquirer": "^7.1.0",
    "ora": "^4.0.3",
    "sequelize": "^5.3.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1",
    "globby": "^11.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/alessandrojean/m3u8-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "m3u8-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "alessandrojean/m3u8-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "build": "mkdir -p public && cp pages/* public && ./bin/run export"
  }
}
